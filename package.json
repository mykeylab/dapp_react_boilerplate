{
  "name": "degate",
  "version": "1.0.0",
  "description": "degate h5",
  "author": "guangliang.chen",
  "main": "server.js",
  "moduleRoots": ".",
  "scripts": {
    "postinstall": "npm run build:dll",
    "start": "cross-env NODE_ENV=development node server.js",
    "build:dll": "node config/dependencies.js",
    "deploy": "cross-env NODE_ENV=production `npm bin`/webpack --config ./config/webpack.config.production.js && npm start",
    "clean:production": "rm -rf build && npm install && npm run build",
    "build": "cross-env NODE_ENV=production webpack -p --config ./config/webpack.config.production.js --progress",
    "lint:eslint": "eslint --ignore-path .gitignore --ignore-pattern static --fix",
    "lint:css": "stylelint './js/**/*.js'",
    "lint:license": "node ./config/license"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run lint:eslint",
      "git add"
    ]
  },
